    "@types/shelljs": "^0.8.15",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@unocss/core": "^0.58.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "detect-node": "^2.1.0",
    "eslint": "^8.56.0",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.0",
    "markdownlint-cli": "^0.38.0",
    "prettier": "^3.1.1",
