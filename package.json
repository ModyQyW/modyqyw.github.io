  "scripts": {
    "build": "vitepress build docs",
    "check:deps": "taze",
    "commit": "git-cz",
    "dev": "vitepress dev docs",
    "preview": "vitepress preview docs",
    "lint": "run-p lint:eslint lint:markdownlint lint:stylelint",
    "lint:eslint": "eslint . --fix --cache --no-error-on-unmatched-pattern --ignore-path=.gitignore",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,scss,vue}\" --fix --cache --allow-empty-input --ignore-path=.gitignore",
    "prepare": "is-ci || simple-git-hooks"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@commitlint/prompt": "^17.6.5",
    "@modyqyw/fabric": "^8.3.1",
    "@types/detect-node": "^2.0.0",
    "@types/fabric": "^5.3.3",
    "@types/shelljs": "^0.8.12",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.8",
    "detect-node": "^2.1.0",
    "element-plus": "^2.3.6",
    "eslint": "^8.43.0",
    "fabric": "^5.3.0",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.2.2",
    "markdownlint-cli": "^0.34.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "shelljs": "^0.8.5",
    "simple-git-hooks": "^2.8.1",
    "sitemap": "^7.1.1",
    "stylelint": "^15.8.0",
    "taze": "^0.10.2",
    "typescript": "^5.1.3",
    "unocss": "^0.53.1",
    "viewerjs": "^1.11.3",
    "vite": "^4.3.9",
    "vitepress": "1.0.0-beta.2",
    "vitepress-plugin-comment-with-giscus": "^1.1.7",
    "vitepress-plugin-image-viewer": "^1.1.2",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.0"
  },
  "packageManager": "pnpm@8.6.3",
  "engines": {
    "node": ">= 14.18"
