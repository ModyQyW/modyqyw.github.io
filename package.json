  "scripts": {
    "build": "vuepress build docs",
    "commit": "cz",
    "dev": "vuepress dev docs",
    "dev:vitepress": "vitepress dev docs",
    "lint": "run-p lint:eslint lint:markdownlint lint:stylelint",
    "lint:eslint": "eslint . --fix --ext=.js,.cjs,.mjs,.jsx,.ts,.cts,.mts,.tsx,.vue,.svelte --ignore-path=.gitignore --cache",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:stylelint": "stylelint \"./**/*.{css,less,scss,sass,vue,svelte}\" --fix --allow-empty-input --ignore-path=.gitignore --cache",
    "prepare": "is-ci || husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@commitlint/prompt": "^17.3.0",
    "@modyqyw/fabric": "^5.14.0",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@vuepress/client": "2.0.0-beta.59",
    "@vuepress/plugin-docsearch": "2.0.0-rc.0",
    "@vuepress/plugin-search": "2.0.0-beta.59",
    "@vuepress/plugin-shiki": "2.0.0-beta.59",
    "@vuepress/theme-default": "2.0.0-beta.59",
    "commitizen": "^4.2.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.30.0",
    "husky": "^8.0.2",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.0",
    "markdownlint-cli": "^0.32.2",
    "mermaid": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "stylelint": "^14.16.0",
    "typescript": "^4.9.4",
    "vitepress": "1.0.0-alpha.33",
    "vitepress-plugin-mermaid": "^2.0.8",
    "vuepress": "2.0.0-beta.59",
    "vuepress-plugin-copy-code2": "2.0.0-beta.145",
    "vuepress-plugin-seo2": "2.0.0-beta.145",
    "vuepress-plugin-sitemap2": "2.0.0-beta.145"
  }
